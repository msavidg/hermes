@{
    ViewBag.Title = "Home Page";
}

<script type="text/javascript">
    function GetTime() {

        $.ajax(
            {
                method: 'GET',
                url: '/api/Data/SendMessage',
                success: function (data, textStatus, jqXHR) {
                    var control = $("#result");
                    control[0].innerHTML = data;
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    window.alert(errorThrown);
                }
            });
    }
</script>
<div class="ml-2 mt-2">
    <div id="result" name="result">No Data</div>
    <div><input type="button" onclick="javascript: GetTime();" value="Call" /></div>
</div>